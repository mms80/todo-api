{
    "name": "mms80/todo-api",
    "description": "todo api",
    "license": "MIT",
    "type": "library",
    "authors": [{
        "name": "Mahdi Sedighian",
        "email": "m.mahdi.sedighian@gmail.com"
    }],
    "require-dev": {
        "orchestra/testbench": "^5.8",
        "laravel/legacy-factories": "^1.0.4"
    },
    "autoload": {
        "psr-4": {
